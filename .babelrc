{
  "presets": ["es2015", "stage-0", "stage-1", "react"],
  "env": {
    "development": {
      "plugins": [
        'syntax-dynamic-import',
        [
            'react-css-modules',
            {
                generateScopedName: '[name]__[local]___[hash:base64:5]',
            },
        ],
        'transform-decorators-legacy',
        'transform-class-properties',
        'transform-object-rest-spread'
      ]
    }
  }
}
